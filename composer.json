{
    "name": "georgpreissl/contao-grix-bundle",
    "description": "Ein Bootstrap-Sitebuilder f√ºr Contao",
    "keywords": ["contao","grid","sitebuilder","bootstrap","columns","spalten"],
    "type": "contao-bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Georg Preissl",
            "email": "g.preissl@heidlmair.com",
            "homepage": "https://www.georg-preissl.at",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/georgpreissl/contao-grix-bundle/issues",
        "source": "https://github.com/georgpreissl/contao-grix-bundle"
    },
    "require": {
        "contao/core-bundle": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "GeorgPreissl\\ContaoGrixBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages",
            "src/Resources/contao/templates"
        ]
    },
    "extra": {
        "contao-manager-plugin": "GeorgPreissl\\ContaoGrixBundle\\ContaoManager\\Plugin"
    }
}
